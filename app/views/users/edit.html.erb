<h4>Edit account</h4>
 <% if @user.errors.any? %>
 <div id="error_explanation">
 <h2><%= pluralize(@user.errors.count, "error") %> prohibited this <%= @user.name %> from being saved:</h2>
 <ul>
 <% @user.errors.full_messages.each do |message| %>
 <li><%= message %></li>
 <% end %>
 </ul>
 </div>
 <% end %>
 <%= form_with(model: @user, local: true) do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>
 <%= f.label :email %>
 <%= f.email_field :email %>
 <%= f.label :profile %>
 <%= f.file_field :profile %>
 <%= f.hidden_field :profile_cache %>
 <button type="button" class="Submit"><%= f.submit "Submit" %></button>
 <% end %>
