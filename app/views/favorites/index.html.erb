<p id="notice"><%= notice %></p>

<h1>My favorites</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Posted by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favorites.each do |f| %>
      <tr>
        <td><%=image_tag f.feed.image.url%></td>
          <td><%= f.feed.user.name %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>


<button type="button" class="btn btn-success"><%= link_to 'Return to the feeds', feeds_path %></button>
